{
  "name": "cjs-package",
  "version": "0.0.0",
  "type": "commonjs",
  "description": "A Node.js CJS package.",
  "keywords": [
    "node",
    "cjs"
  ],
  "author": "moon",
  "license": "MIT",
  "repository": "https://github.com/moonrepo/examples",
  "packemon": {
    "format": "cjs",
    "platform": "node",
    "inputs": {
      "index": "src/index.cts"
    }
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.cts",
    "src/**/*.{cts,json}"
  ],
  "types": "./dts/index.d.cts",
  "main": "./cjs/index.cjs",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.cts",
      "node": {
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.cts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  }
}
